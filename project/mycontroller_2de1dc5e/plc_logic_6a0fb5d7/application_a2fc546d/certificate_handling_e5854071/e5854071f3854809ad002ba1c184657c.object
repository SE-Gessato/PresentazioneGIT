_timTimeout:= T#10S, q_xBusy=> , q_xDone=> xCertificateSelection, q_xError=> ) THEN\n// \t\t\t\t\tFB_CH.Delete(i_timTimeout:= T#5S, q_xBusy=> , q_xDone=> , q_xError=> );\t\t\t\t\t\t\t\t\t\t\t\t\t\t//elimino dalla quarantena\n// \t\t\t\tEND_IF\n// \t\t\tEND_IF\n// \t\tEND_IF\n// \tEND_IF\n// END_IF\n\n\nIF udicase=0 THEN\n\tFB_CH.SelectFirstCertificate(i_etTrustLevel:= 3, i_timTimeout:= T#10S, q_xBusy=> xBusy, q_xDone=> xDoneSCQ, q_xError=> xErrorSCQ);\n\tIF xDoneSCQ THEN\n\t\tudicase:=5;\n\tEND_IF\n\tIF xErrorSCQ THEN\n\t\tudicase:=1000;\n\tEND_IF\nEND_IF\n\nIF udicase=5 THEN\n\tIF fb_ch.IsCertificateSelected THEN\n\t\tudicase:=10;\n\tEND_IF\nEND_IF\n\nIF udicase =10 THEN\n\tFB_CH.SelectFirstCertificate(i_etTrustLevel:= 1, i_timTimeout:= T#10S, q_xBusy=> , q_xDone=> xDoneSCT, q_xError=> xErrorSCT);\n\tIF xDoneSCT OR  xErrorSCT THEN\n\t\tIF fb_ch.IsCertificateSelected THEN\n\t\t\tudicase:=12;\n\t\tELSE\n\t\t\tIF FB_CH.ReleaseCertificate() THEN\n\t\t\t\tudicase:=20;\n\t\t\tEND_IF\n\t\tEND_IF\n\tEND_IF\nEND_IF\n\nIF udicase =12 THEN\n\tFB_CH.Delete(i_timTimeout:= T#10S, q_xBusy=> , q_xDone=> xDoneDT, q_xError=> xErrorDT);\n\tIF xDoneDT OR  xErrorDT THEN\n\t\tudicase:=10;\n\tEND_IF\nEND_IF\n\n\n\nIF udicase =20 THEN\n\tFB_CH.SelectFirstCertificate(i_etTrustLevel:= 3, i_timTimeout:= T#10S, q_xBusy=> , q_xDone=> xDoneSCQ, q_xError=> xErrorSCQ);\n\tIF xDoneSCQ OR  xErrorSCQ THEN\n\t\tudicase:=25;\n\tEND_IF\nEND_IF\t\n\nIF udicase=25 THEN\n\tIF fb_ch.IsCertificateSelected THEN\n\t\tFB_CH.Copy(i_etDestinationTrustLevel:= 1, i_timTimeout:= T#10S, q_xBusy=> , q_xDone=> xDoneC, q_xError=> xErrorC);\n\t\tIF xDoneC OR xErrorC THEN\n\t\t\t udicase:=27;\n\t\tEND_IF\n\tEND_IF\nEND_IF\n\nIF udicase=27 THEN\n\tIF FB_CH.ReleaseCertificate() THEN\n\t\tudicase:=30;\n\tEND_IF\nEND_IF\n\n\t\t\t\n\t\t\t\nIF udicase =30 THEN\n\tFB_CH.SelectFirstCertificate(i_etTrustLevel:= 3, i_timTimeout:= T#10S, q_xBusy=> , q_xDone=> xDoneSCQ, q_xError=> xErrorSCQ);\n\tIF xDoneSCQ OR xErrorSCQ THEN\n\t\tudicase :=35;\n\tEND_IF\nEND_IF\n\nIF udicase =35 THEN\n\tFB_CH.Delete(i_timTimeout:= T#10S, q_xBusy=> , q_xDone=> xDoneDQ, q_xError=> xErrorDQ);\n\tIF xDoneDQ OR xErrorDQ THEN\n\t\tIF FB_CH.ReleaseCertificate() THEN\n\t\t\tudicase:=0;\n\t\tEND_IF\n\tEND_IF\t\nEND_IF\n\n\nIF udicase =1000 THEN\n\t udicase :=0;\nEND_IF","LineInfoPersistence":"(string)e5854071-f385-4809-ad00-2ba1c184657c_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)PROGRAM Certificate_Handling\nVAR\n// \topen : file.diropen;\n// \tclose: file.DirClose;\n// \tcopy : file.DirCopy;\n// \tremove_quarantena: file.DirRemove;\n// \tremove_trusted: file.DirRemove;\n\t\nFB_CH : se_cert.FB_CertificateHandling;\nF_Blink_Certificate: BLINK;\nxDoneSCT : BOOL;\nxDoneSCQ : BOOL;\nxDoneC : BOOL;\nxDoneDT : BOOL;\nxDoneDQ : BOOL;\n\nxErrorSCT : BOOL;\nxErrorSCQ : BOOL;\nxErrorC : BOOL;\nxErrorDT : BOOL;\nxErrorDQ : BOOL;\n\nxBusy :BOOL;\n\nudicase: INT;\nrisultato_comunicazione : se_cert.et_result;\nstato_comunicazione :  se_cert.et_state;\nEND_VAR\n","LineInfoPersistence":"(string)e5854071-f385-4809-ad00-2ba1c184657c_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)289","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}